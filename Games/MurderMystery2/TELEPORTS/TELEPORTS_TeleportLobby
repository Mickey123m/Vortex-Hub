local Players = game:GetService("Players")
local Workspace = game:GetService("Workspace")

local function teleportToLobby()
    local player = Players.LocalPlayer
    local character = player.Character
    local root = character and character:FindFirstChild("HumanoidRootPart")
    
    -- Posição exata fornecida
    local targetPos = Vector3.new(-5060.4384765625, 284.6402893066406, 77.59838104248047)

    if root then
        -- Teleporta o jogador para a posição exata com um pequeno ajuste de altura para evitar colisão com o chão
        root.CFrame = CFrame.new(targetPos + Vector3.new(0, 3, 0))
        print("Teleportado para a posição do Lobby com sucesso.")
    else
        if not root then 
            warn("Personagem ou HumanoidRootPart não encontrado.") 
        end
    end
end

-- Executa o teleport
teleportToLobby()