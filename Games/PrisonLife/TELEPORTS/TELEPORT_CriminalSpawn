-- Teleport to Criminal Spawn
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local function TeleportCriminalSpawn()
    print("[Prison Life] Teleporting to Criminal Spawn...")
    
    local char = LocalPlayer.Character
    local humanoidRootPart = char and char:FindFirstChild("HumanoidRootPart")
    
    if not humanoidRootPart then
        print("[Prison Life] Character not found!")
        return "Error: Character not found"
    end
    
    -- Coordinates for criminal spawn
    local targetCFrame = CFrame.new(-920, 95, 2138)
    
    -- Teleport
    humanoidRootPart.CFrame = targetCFrame
    task.wait(0.1)
    
    print("[Prison Life] Successfully teleported to Criminal Spawn!")
    return "Teleported to Criminal Spawn"
end

return TeleportCriminalSpawn