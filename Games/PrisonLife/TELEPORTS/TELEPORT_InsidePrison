-- Teleporte seguro para Inside Prison (funciona para criminosos)
local player = game.Players.LocalPlayer
local character = player.Character

-- Método 1: Teleporta em cima e desce
local prisonPosition = Vector3.new(919.857, 99.990, 2379.037)
local rootPart = character:FindFirstChild("HumanoidRootPart")

if rootPart then
    -- Teleporta 50 studs acima
    rootPart.CFrame = CFrame.new(prisonPosition + Vector3.new(0, 50, 0))
    wait(0.1)
    -- Desce para posição normal
    rootPart.CFrame = CFrame.new(prisonPosition)
    print("✅ Teleportado para Inside Prison")
end