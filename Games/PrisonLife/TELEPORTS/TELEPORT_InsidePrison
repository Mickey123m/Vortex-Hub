-- Teleport to Inside Prison
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local function TeleportInsidePrison()
    print("[Prison Life] Teleporting to Inside Prison...")
    
    local char = LocalPlayer.Character
    local humanoidRootPart = char and char:FindFirstChild("HumanoidRootPart")
    
    if not humanoidRootPart then
        print("[Prison Life] Character not found!")
        return "Error: Character not found"
    end
    
    -- Coordinates for inside prison (yard)
    local targetCFrame = CFrame.new(919, 100, 2379)
    
    -- Teleport
    humanoidRootPart.CFrame = targetCFrame
    task.wait(0.1)
    
    print("[Prison Life] Successfully teleported to Inside Prison!")
    return "Teleported to Inside Prison"
end

return TeleportInsidePrison