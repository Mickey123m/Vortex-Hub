-- Teleport to Yard Center
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local function TeleportYardCenter()
    print("[Prison Life] Teleporting to Yard Center...")
    
    local char = LocalPlayer.Character
    local humanoidRootPart = char and char:FindFirstChild("HumanoidRootPart")
    
    if not humanoidRootPart then
        print("[Prison Life] Character not found!")
        return "Error: Character not found"
    end
    
    -- Coordinates for yard center
    local targetCFrame = CFrame.new(789, 98, 2475)
    
    -- Teleport
    humanoidRootPart.CFrame = targetCFrame
    task.wait(0.1)
    
    print("[Prison Life] Successfully teleported to Yard Center!")
    return "Teleported to Yard Center"
end

return TeleportYardCenter