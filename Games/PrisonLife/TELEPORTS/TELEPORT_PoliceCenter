-- Teleport to Police Center
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local function TeleportPoliceCenter()
    print("[Prison Life] Teleporting to Police Center...")
    
    local char = LocalPlayer.Character
    local humanoidRootPart = char and char:FindFirstChild("HumanoidRootPart")
    
    if not humanoidRootPart then
        print("[Prison Life] Character not found!")
        return "Error: Character not found"
    end
    
    -- Coordinates for police station
    local targetCFrame = CFrame.new(827, 100, 2288)
    
    -- Teleport
    humanoidRootPart.CFrame = targetCFrame
    task.wait(0.1)
    
    print("[Prison Life] Successfully teleported to Police Center!")
    return "Teleported to Police Center"
end

return TeleportPoliceCenter