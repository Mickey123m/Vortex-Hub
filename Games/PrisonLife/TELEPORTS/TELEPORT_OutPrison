-- Teleport to Outside Prison
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local function TeleportOutPrison()
    print("[Prison Life] Teleporting to Outside Prison...")
    
    local char = LocalPlayer.Character
    local humanoidRootPart = char and char:FindFirstChild("HumanoidRootPart")
    
    if not humanoidRootPart then
        print("[Prison Life] Character not found!")
        return "Error: Character not found"
    end
    
    -- Coordinates for outside prison
    local targetCFrame = CFrame.new(400, 100, 2215)
    
    -- Teleport
    humanoidRootPart.CFrame = targetCFrame
    task.wait(0.1)
    
    print("[Prison Life] Successfully teleported to Outside Prison!")
    return "Teleported to Outside Prison"
end

return TeleportOutPrison