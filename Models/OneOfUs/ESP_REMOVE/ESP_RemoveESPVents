-- Script para Desativar ESP Vents
_G.VentsESP_Enabled = false

local workspace = game:GetService("Workspace")
local map = workspace:FindFirstChild("map")

if map then
    local ventsFolder = map:FindFirstChild("Vents")
    if ventsFolder then
        for _, vent in ipairs(ventsFolder:GetChildren()) do
            local highlight = vent:FindFirstChild("ESP_Highlight_Vent")
            if highlight then
                highlight:Destroy()
            end
        end
    end
end

print("ESP Vents Desativado")