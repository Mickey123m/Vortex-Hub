shared.AnimalESP_Enabled = false

local Workspace = game:GetService("Workspace")
local animalsFolder = Workspace:FindFirstChild("Animals")
if not animalsFolder then return end

for _, animal in ipairs(animalsFolder:GetChildren()) do
	local h = animal:FindFirstChild("AnimalHighlight")
	if h then h:Destroy() end

	local b = animal:FindFirstChild("AnimalDistanceGui")
	if b then b:Destroy() end
end

shared.AnimalESP_Data = {}

print("[ANIMAL ESP] REMOVIDO")
