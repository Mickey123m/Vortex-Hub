        local value = validateNumberInput(ThiefBrainrotValueInput)
        currentThiefSettings.MinValue = value
    end)

    local function StartThiefBrainrot()
        if thiefBrainrotActive then return end
        
        local minValue = validateNumberInput(ThiefBrainrotValueInput)
        
        if minValue <= 0 then
            modules.ShowNotification("Error", "Enter a valid minimum value!", 3)
            return
        end
        
        thiefBrainrotActive = true
        TweenService:Create(ThiefBrainrotSwitch, TweenInfo.new(0.3), {BackgroundColor3 = CategoryColors.AutoFarm}):Play()
        TweenService:Create(ThiefBrainrotCircle, TweenInfo.new(0.3), {Position = UDim2.new(1, -17, 0.5, -7), BackgroundColor3 = Color3.new(1, 1, 1)}):Play()
        
        ThiefBrainrotValueInput.TextEditable = false
        ThiefBrainrotValueInput.TextColor3 = Color3.fromRGB(180, 180, 180)
        
        _G.ValorAlvoBounty = minValue
        
        -- Registrar script ativo
        AutoThiefScript = {
            Deactivate = function()
                _G.BountyHunter = false
                _G.ValorAlvoBounty = nil
            end
        }
        
        task.spawn(function()
            local scriptUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/BreakALuckyBlock/AUTOFARM/AUTO_BrainrotThief"
            
            local success, err = pcall(function()
                local source = game:HttpGet(scriptUrl)
                if source then
                    local func = loadstring(source)
                    if func then
                        func()
                    else
                        error("Failed to loadstring")
                    end
                else
                    error("Failed to get script source")
                end
            end)
            
            if not success then
                modules.ShowNotification("Error", "Failed to load thief script", 4)
                
                thiefBrainrotActive = false
                TweenService:Create(ThiefBrainrotSwitch, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(60, 60, 75)}):Play()
                TweenService:Create(ThiefBrainrotCircle, TweenInfo.new(0.3), {Position = UDim2.new(0, 3, 0.5, -7), BackgroundColor3 = Color3.fromRGB(200, 200, 200)}):Play()
                
                ThiefBrainrotValueInput.TextEditable = true
                ThiefBrainrotValueInput.TextColor3 = CurrentTextColor
                
                AutoThiefScript = nil
            end
        end)
    end

    local function StopThiefBrainrot()
        if not thiefBrainrotActive then return end
        
        thiefBrainrotActive = false
        TweenService:Create(ThiefBrainrotSwitch, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(60, 60, 75)}):Play()
        TweenService:Create(ThiefBrainrotCircle, TweenInfo.new(0.3), {Position = UDim2.new(0, 3, 0.5, -7), BackgroundColor3 = Color3.fromRGB(200, 200, 200)}):Play()
        
        ThiefBrainrotValueInput.TextEditable = true
        ThiefBrainrotValueInput.TextColor3 = CurrentTextColor
        
        -- Desativar script
        if AutoThiefScript then
            AutoThiefScript.Deactivate()
            AutoThiefScript = nil
        end
    end

    ThiefBrainrotSwitch.MouseButton1Click:Connect(function()
        if thiefBrainrotActive then
            StopThiefBrainrot()
        else
            StartThiefBrainrot()
        end
    end)

