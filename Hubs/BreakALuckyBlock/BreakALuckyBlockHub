    ValueInputPadding.PaddingLeft = UDim.new(0, 8)
    ValueInputPadding.PaddingRight = UDim.new(0, 8)

    local BrainrotWebhookContainer = Instance.new("Frame", BrainrotContainer)
    BrainrotWebhookContainer.Size = UDim2.new(1, -30, 0, 35)
    BrainrotWebhookContainer.Position = UDim2.new(0, 15, 0, 120)
    BrainrotWebhookContainer.BackgroundTransparency = 1

    local BrainrotWebhookLabel = Instance.new("TextLabel", BrainrotWebhookContainer)
    BrainrotWebhookLabel.Size = UDim2.new(0.5, 0, 1, 0)
    BrainrotWebhookLabel.Position = UDim2.new(0, 0, 0, 0)
    BrainrotWebhookLabel.Text = "Webhook URL (Optional)"
    BrainrotWebhookLabel.TextColor3 = CurrentTextColor
    BrainrotWebhookLabel.Font = Enum.Font.GothamMedium
    BrainrotWebhookLabel.TextSize = 14
    BrainrotWebhookLabel.TextXAlignment = Enum.TextXAlignment.Left
    BrainrotWebhookLabel.BackgroundTransparency = 1

    local BrainrotWebhookInput = Instance.new("TextBox", BrainrotWebhookContainer)
    BrainrotWebhookInput.Size = UDim2.new(0.45, -15, 0, 28)
    BrainrotWebhookInput.Position = UDim2.new(0.55, 0, 0.5, -14)
    BrainrotWebhookInput.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
    BrainrotWebhookInput.TextColor3 = CurrentTextColor
    BrainrotWebhookInput.Font = Enum.Font.GothamMedium
    BrainrotWebhookInput.TextSize = 12
    BrainrotWebhookInput.PlaceholderText = "Leave blank if you don't want to."
    BrainrotWebhookInput.PlaceholderColor3 = Color3.fromRGB(150, 150, 150)
    BrainrotWebhookInput.Text = ""
    BrainrotWebhookInput.ClearTextOnFocus = false
    BrainrotWebhookInput.TextXAlignment = Enum.TextXAlignment.Left
    BrainrotWebhookInput.TextTruncate = Enum.TextTruncate.AtEnd
    Instance.new("UICorner", BrainrotWebhookInput).CornerRadius = UDim.new(0, 6)

    local WebhookInputPadding = Instance.new("UIPadding", BrainrotWebhookInput)
    WebhookInputPadding.PaddingLeft = UDim.new(0, 8)
    WebhookInputPadding.PaddingRight = UDim.new(0, 8)

    local brainrotActive = false
    local currentFarmSettings = {
        MinValue = 100,
        WebhookURL = ""
    }

    local function validateNumberInput(input)
        local text = input.Text
        text = text:gsub("%s+", "")
        
        if text == "" then
            input.Text = ""
            return 0
        end
        
        local value = tonumber(text)
        if not value then
            input.Text = ""
            return 0
        end
        
        if value < 0 then
            input.Text = ""
            return 0
        end
        
        input.Text = tostring(math.floor(value))
        return math.floor(value)
    end

    local function validateWebhookInput()
        local text = BrainrotWebhookInput.Text
        text = text:gsub("%s+", "")
        
        if text == "" then
            currentFarmSettings.WebhookURL = ""
            return ""
        end
        
        if not (text:find("discord.com/api/webhooks") or text:find("discordapp.com/api/webhooks")) then
            BrainrotWebhookInput.Text = ""
            currentFarmSettings.WebhookURL = ""
            return ""
        end
        
        currentFarmSettings.WebhookURL = text
        return text
    end

    BrainrotValueInput.FocusLost:Connect(function()
        local value = validateNumberInput(BrainrotValueInput)
        currentFarmSettings.MinValue = value
    end)

    BrainrotWebhookInput.FocusLost:Connect(function()
        validateWebhookInput()
    end)

    local function StartBrainrotFarm()
        if brainrotActive then return end
        
        local minValue = validateNumberInput(BrainrotValueInput)
        local webhook = validateWebhookInput()
        
        if minValue <= 0 then
            modules.ShowNotification("Error", "Enter a valid minimum value!", 3)
            return
        end
        
        brainrotActive = true
        TweenService:Create(BrainrotSwitch, TweenInfo.new(0.3), {BackgroundColor3 = CategoryColors.AutoFarm}):Play()
        TweenService:Create(BrainrotCircle, TweenInfo.new(0.3), {Position = UDim2.new(1, -17, 0.5, -7), BackgroundColor3 = Color3.new(1, 1, 1)}):Play()
        
        BrainrotValueInput.TextEditable = false
        BrainrotValueInput.TextColor3 = Color3.fromRGB(180, 180, 180)
        BrainrotWebhookInput.TextEditable = false
        BrainrotWebhookInput.TextColor3 = Color3.fromRGB(180, 180, 180)
        
        _G.ValorMinimo = minValue
        _G.WebhookURL = webhook
        _G.DiscordID = ""
        
        -- Registrar script ativo
        AutoFarmScripts["BrainrotFarm"] = {
            Deactivate = function()
                if _G.VortexStopFarm then
                    pcall(_G.VortexStopFarm)
                end
                _G.ValorMinimo = nil
                _G.WebhookURL = nil
                _G.DiscordID = nil
            end
        }
        
        task.spawn(function()
            local scriptUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/BreakALuckyBlock/AUTOFARM/AUTO_AutoFarmBrainrot"
            
            local success, err = pcall(function()
                loadstring(game:HttpGet(scriptUrl))()
            end)
            
            if not success then
                modules.ShowNotification("Error", "Failed to load farm script: " .. tostring(err), 4)
                
                brainrotActive = false
                TweenService:Create(BrainrotSwitch, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(60, 60, 75)}):Play()
                TweenService:Create(BrainrotCircle, TweenInfo.new(0.3), {Position = UDim2.new(0, 3, 0.5, -7), BackgroundColor3 = Color3.fromRGB(200, 200, 200)}):Play()
                
                BrainrotValueInput.TextEditable = true
                BrainrotValueInput.TextColor3 = CurrentTextColor
                BrainrotWebhookInput.TextEditable = true
                BrainrotWebhookInput.TextColor3 = CurrentTextColor
                
                AutoFarmScripts["BrainrotFarm"] = nil
            end
        end)
    end

    local function StopBrainrotFarm()
        if not brainrotActive then return end
        
        brainrotActive = false
        TweenService:Create(BrainrotSwitch, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(60, 60, 75)}):Play()
        TweenService:Create(BrainrotCircle, TweenInfo.new(0.3), {Position = UDim2.new(0, 3, 0.5, -7), BackgroundColor3 = Color3.fromRGB(200, 200, 200)}):Play()
        
        BrainrotValueInput.TextEditable = true
        BrainrotValueInput.TextColor3 = CurrentTextColor
        BrainrotWebhookInput.TextEditable = true
        BrainrotWebhookInput.TextColor3 = CurrentTextColor
        
        -- Desativar script
        if AutoFarmScripts["BrainrotFarm"] then
            AutoFarmScripts["BrainrotFarm"].Deactivate()
            AutoFarmScripts["BrainrotFarm"] = nil
        end
        
        _G.ValorMinimo = nil
        _G.WebhookURL = nil
        _G.DiscordID = nil
    end

    BrainrotSwitch.MouseButton1Click:Connect(function()
        if brainrotActive then
            StopBrainrotFarm()
        else
            StartBrainrotFarm()
        end
    end)

    -- AUTO REBIRTH (SEGUNDO - ABAIXO DO BRAINROT)
    local RebirthContainer = Instance.new("Frame", AutoFarmContainer)
    RebirthContainer.Size = UDim2.new(1, 0, 0, 140)
    RebirthContainer.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    RebirthContainer.BackgroundTransparency = 0
    Instance.new("UICorner", RebirthContainer).CornerRadius = UDim.new(0, 8)

    local RebirthTitle = Instance.new("TextLabel", RebirthContainer)
    RebirthTitle.Size = UDim2.new(1, -30, 0, 35)
    RebirthTitle.Position = UDim2.new(0, 15, 0, 5)
    RebirthTitle.Text = "Auto Rebirth"
    RebirthTitle.TextColor3 = CurrentTextColor
    RebirthTitle.Font = Enum.Font.GothamBold
    RebirthTitle.TextSize = 16
    RebirthTitle.TextXAlignment = Enum.TextXAlignment.Left
    RebirthTitle.BackgroundTransparency = 1

    local RebirthToggleFrame = Instance.new("Frame", RebirthContainer)
    RebirthToggleFrame.Size = UDim2.new(1, -30, 0, 35)
    RebirthToggleFrame.Position = UDim2.new(0, 15, 0, 40)
    RebirthToggleFrame.BackgroundTransparency = 1

    local RebirthToggleLabel = Instance.new("TextLabel", RebirthToggleFrame)
    RebirthToggleLabel.Size = UDim2.new(0.7, 0, 1, 0)
    RebirthToggleLabel.Position = UDim2.new(0, 0, 0, 0)
    RebirthToggleLabel.Text = "Activate Auto Rebirth"
    RebirthToggleLabel.TextColor3 = CurrentTextColor
    RebirthToggleLabel.Font = Enum.Font.GothamMedium
    RebirthToggleLabel.TextSize = 14
    RebirthToggleLabel.TextXAlignment = Enum.TextXAlignment.Left
    RebirthToggleLabel.BackgroundTransparency = 1

    local RebirthSwitch = Instance.new("TextButton", RebirthToggleFrame)
    RebirthSwitch.Size = UDim2.new(0, 38, 0, 20)
    RebirthSwitch.Position = UDim2.new(1, -38, 0.5, -10)
    RebirthSwitch.BackgroundColor3 = Color3.fromRGB(60, 60, 75)
    RebirthSwitch.Text = ""
    Instance.new("UICorner", RebirthSwitch).CornerRadius = UDim.new(1, 0)

    local RebirthCircle = Instance.new("Frame", RebirthSwitch)
    RebirthCircle.Size = UDim2.new(0, 14, 0, 14)
    RebirthCircle.Position = UDim2.new(0, 3, 0.5, -7)
    RebirthCircle.BackgroundColor3 = Color3.fromRGB(200, 200, 200)
    Instance.new("UICorner", RebirthCircle).CornerRadius = UDim.new(1, 0)

    local RebirthValueContainer = Instance.new("Frame", RebirthContainer)
    RebirthValueContainer.Size = UDim2.new(1, -30, 0, 35)
    RebirthValueContainer.Position = UDim2.new(0, 15, 0, 80)
    RebirthValueContainer.BackgroundTransparency = 1

    local RebirthValueLabel = Instance.new("TextLabel", RebirthValueContainer)
    RebirthValueLabel.Size = UDim2.new(0.5, 0, 1, 0)
    RebirthValueLabel.Position = UDim2.new(0, 0, 0, 0)
    RebirthValueLabel.Text = "Minimum Cash Value"
    RebirthValueLabel.TextColor3 = CurrentTextColor
    RebirthValueLabel.Font = Enum.Font.GothamMedium
    RebirthValueLabel.TextSize = 14
    RebirthValueLabel.TextXAlignment = Enum.TextXAlignment.Left
    RebirthValueLabel.BackgroundTransparency = 1

    local RebirthValueInput = Instance.new("TextBox", RebirthValueContainer)
    RebirthValueInput.Size = UDim2.new(0.45, -15, 0, 28)
    RebirthValueInput.Position = UDim2.new(0.55, 0, 0.5, -14)
    RebirthValueInput.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
    RebirthValueInput.TextColor3 = CurrentTextColor
    RebirthValueInput.Font = Enum.Font.GothamMedium
    RebirthValueInput.TextSize = 13
    RebirthValueInput.PlaceholderText = "Ex: 5000"
    RebirthValueInput.PlaceholderColor3 = Color3.fromRGB(150, 150, 150)
    RebirthValueInput.Text = "5000"
    RebirthValueInput.ClearTextOnFocus = false
    Instance.new("UICorner", RebirthValueInput).CornerRadius = UDim.new(0, 6)

    local RebirthInputPadding = Instance.new("UIPadding", RebirthValueInput)
    RebirthInputPadding.PaddingLeft = UDim.new(0, 8)
    RebirthInputPadding.PaddingRight = UDim.new(0, 8)

    local rebirthActive = false
    local currentRebirthSettings = {
        MinValue = 5000
    }

    RebirthValueInput.FocusLost:Connect(function()
        local value = validateNumberInput(RebirthValueInput)
        currentRebirthSettings.MinValue = value
    end)

    local function StartAutoRebirth()
        if rebirthActive then return end
        
        local minValue = validateNumberInput(RebirthValueInput)
        
        if minValue <= 0 then
            modules.ShowNotification("Error", "Enter a valid minimum cash value!", 3)
            return
        end
        
        rebirthActive = true
        TweenService:Create(RebirthSwitch, TweenInfo.new(0.3), {BackgroundColor3 = CategoryColors.AutoRebirth}):Play()
        TweenService:Create(RebirthCircle, TweenInfo.new(0.3), {Position = UDim2.new(1, -17, 0.5, -7), BackgroundColor3 = Color3.new(1, 1, 1)}):Play()
        
        RebirthValueInput.TextEditable = false
        RebirthValueInput.TextColor3 = Color3.fromRGB(180, 180, 180)
        
        -- Configurar as variáveis globais que o script de Auto Rebirth espera
        _G.ValorMinimo = minValue
        
        -- Registrar script ativo
        AutoRebirthScript = {
            Deactivate = function()
                _G.AutoRebirth = false
                if _G.UltimateAutoFarm then
                    _G.UltimateAutoFarm = false
                end
                _G.ValorMinimo = nil
            end
        }
        
        task.spawn(function()
            local scriptUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/BreakALuckyBlock/AUTOFARM/AUTO_AutoRebirth"
            
            local success, err = pcall(function()
                loadstring(game:HttpGet(scriptUrl))()
            end)
            
            if not success then
                modules.ShowNotification("Error", "Failed to load Auto Rebirth script: " .. tostring(err), 4)
                
                rebirthActive = false
                TweenService:Create(RebirthSwitch, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(60, 60, 75)}):Play()
                TweenService:Create(RebirthCircle, TweenInfo.new(0.3), {Position = UDim2.new(0, 3, 0.5, -7), BackgroundColor3 = Color3.fromRGB(200, 200, 200)}):Play()
                
                RebirthValueInput.TextEditable = true
                RebirthValueInput.TextColor3 = CurrentTextColor
                
                AutoRebirthScript = nil
            else
                modules.ShowNotification("Success", "Auto Rebirth started with min value: " .. minValue, 3)
            end
        end)
    end

    local function StopAutoRebirth()
        if not rebirthActive then return end
        
        rebirthActive = false
        TweenService:Create(RebirthSwitch, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(60, 60, 75)}):Play()
        TweenService:Create(RebirthCircle, TweenInfo.new(0.3), {Position = UDim2.new(0, 3, 0.5, -7), BackgroundColor3 = Color3.fromRGB(200, 200, 200)}):Play()
        
        RebirthValueInput.TextEditable = true
        RebirthValueInput.TextColor3 = CurrentTextColor
        
        -- Parar o script de Auto Rebirth
        if AutoRebirthScript then
            AutoRebirthScript.Deactivate()
            AutoRebirthScript = nil
        end
        
        modules.ShowNotification("Success", "Auto Rebirth stopped", 3)
    end

    RebirthSwitch.MouseButton1Click:Connect(function()
        if rebirthActive then
            StopAutoRebirth()
        else
            StartAutoRebirth()
        end
    end)

