            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/ESP_REMOVE/ESP_RemoveESPSurvivors")
            OneOfUs.ESP_Survivors_Active = false
        end
        if OneOfUs.ESP_Vents_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/ESP_REMOVE/ESP_RemoveESPVents")
            OneOfUs.ESP_Vents_Active = false
        end
        if OneOfUs.ESP_Windows_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/ESP_REMOVE/ESP_RemoveESPWindows")
            OneOfUs.ESP_Windows_Active = false
        end
        
        if OneOfUs.UTILITIES_AntiFall_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/UTILITIES_REMOVE/UTILITIES_RemoveAntiFall")
            OneOfUs.UTILITIES_AntiFall_Active = false
        end
        if OneOfUs.UTILITIES_AntiTrap_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/UTILITIES_REMOVE/UTILITIES_RemoveAntiTrap")
            OneOfUs.UTILITIES_AntiTrap_Active = false
        end
        if OneOfUs.UTILITIES_AutoGeradors_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/UTILITIES_REMOVE/UTILITIES_RemoveAutoGeradors")
            OneOfUs.UTILITIES_AutoGeradors_Active = false
        end
        if OneOfUs.UTILITIES_AutoHealPlayers_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/UTILITIES_REMOVE/UTILITIES_RemoveAutoHealPlayers")
            OneOfUs.UTILITIES_AutoHealPlayers_Active = false
        end
        if OneOfUs.UTILITIES_AutoInteractions_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/UTILITIES_REMOVE/UTILITIES_RemoveInteractions")
            OneOfUs.UTILITIES_AutoInteractions_Active = false
        end
        if OneOfUs.UTILITIES_AutoSaboteGeradors_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/UTILITIES_REMOVE/UTILITIES_RemoveAutoSaboteGeradors")
            OneOfUs.UTILITIES_AutoSaboteGeradors_Active = false
        end
        if OneOfUs.UTILITIES_DistanceGeradors_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/UTILITIES_REMOVE/UTILITIES_RemoveDistanceGeradors")
            OneOfUs.UTILITIES_DistanceGeradors_Active = false
        end
        if OneOfUs.UTILITIES_Noclip_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/UTILITIES_REMOVE/UTILITIES_RemoveNoclip")
            OneOfUs.UTILITIES_Noclip_Active = false
        end
        if OneOfUs.UTILITIES_FullBrightness_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FNAFEternalNights/UTILITIES_REMOVE/UTILITIES_RemoveFullBrightness")
            OneOfUs.UTILITIES_FullBrightness_Active = false
        end
    end

    if FleeTheFacility then
        if FleeTheFacility.ESP_AirVent_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FleeTheFacility/ESP_REMOVE/ESP_RemoveESPAirVent")
            FleeTheFacility.ESP_AirVent_Active = false
        end
        if FleeTheFacility.ESP_Beast_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FleeTheFacility/ESP_REMOVE/ESP_RemoveESPBeast")
            FleeTheFacility.ESP_Beast_Active = false
        end
        if FleeTheFacility.ESP_Computers_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FleeTheFacility/ESP_REMOVE/ESP_RemoveESPComputers")
            FleeTheFacility.ESP_Computers_Active = false
        end
        if FleeTheFacility.ESP_Exit_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FleeTheFacility/ESP_REMOVE/ESP_RemoveESPExit")
            FleeTheFacility.ESP_Exit_Active = false
        end
        if FleeTheFacility.ESP_FreezePad_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FleeTheFacility/ESP_REMOVE/ESP_RemoveESPFreezePod")
            FleeTheFacility.ESP_FreezePad_Active = false
        end
        if FleeTheFacility.ESP_Name_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FleeTheFacility/ESP_REMOVE/ESP_RemoveESPName")
            FleeTheFacility.ESP_Name_Active = false
        end
        if FleeTheFacility.ESP_Players_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FleeTheFacility/ESP_REMOVE/ESP_RemoveESPPlayers")
            FleeTheFacility.ESP_Players_Active = false
        end
        
        if FleeTheFacility.UTILITIES_AntiRagdoll_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FleeTheFacility/UTILITIES_REMOVE/UTILITIES_RemoveAntiRagdoll")
            FleeTheFacility.UTILITIES_AntiRagdoll_Active = false
        end
        if FleeTheFacility.UTILITIES_FastCrawl_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FleeTheFacility/UTILITIES_REMOVE/UTILITIES_RemoveFastCrawl")
            FleeTheFacility.UTILITIES_FastCrawl_Active = false
        end
        if FleeTheFacility.UTILITIES_InfinityDoubleJump_Active then
            self:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FleeTheFacility/UTILITIES_REMOVE/UTILITIES_RemoveInfinityDoubleJump")
            FleeTheFacility.UTILITIES_InfinityDoubleJump_Active = false
        end
    end
    
    for name, cleanupFunc in pairs(self.ScriptsToClean) do
        pcall(cleanupFunc)
    end
    
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("Highlight") then
            pcall(function() obj:Destroy() end)
        elseif obj:IsA("BillboardGui") and (obj.Name:find("ESP") or obj.Name:find("esp") or obj.Name:find("NomeTag") or obj.Name:find("NameTag")) then
            pcall(function() obj:Destroy() end)
        elseif obj:IsA("Folder") and (obj.Name:find("ESP") or obj.Name:find("esp") or obj.Name:find("Internal")) then
            pcall(function() obj:Destroy() end)
        end
    end
    
    for _, conn in ipairs(self.Connections) do
        if conn and typeof(conn) == "RBXScriptConnection" then
            pcall(function() conn:Disconnect() end)
        end
    end
    self.Connections = {}
    
    self.Running = false
end

local FleeTheFacility = {
    ESP_AirVent_Active = false,
    ESP_Beast_Active = false,
    ESP_Computers_Active = false,
    ESP_Exit_Active = false,
    ESP_FreezePad_Active = false,
    ESP_Name_Active = false,
    ESP_Players_Active = false,
    
    UTILITIES_AntiRagdoll_Active = false,
    UTILITIES_FastCrawl_Active = false,
    UTILITIES_InfinityDoubleJump_Active = false
}

function FleeTheFacility:ToggleESP(espType, state)
    local activateUrl = ""
    local removeUrl = ""
    local stateName = ""
    
    if espType == "AirVent" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/FleeTheFacility/ESP/ESP_AirVent"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FleeTheFacility/ESP_REMOVE/ESP_RemoveESPAirVent"
        stateName = "FTFT_ESP_AirVent"
    elseif espType == "Beast" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/FleeTheFacility/ESP/ESP_Beast"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FleeTheFacility/ESP_REMOVE/ESP_RemoveESPBeast"
        stateName = "FTFT_ESP_Beast"
    elseif espType == "Computers" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/FleeTheFacility/ESP/ESP_Computers"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FleeTheFacility/ESP_REMOVE/ESP_RemoveESPComputers"
        stateName = "FTFT_ESP_Computers"
    elseif espType == "Exit" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/FleeTheFacility/ESP/ESP_Exit"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FleeTheFacility/ESP_REMOVE/ESP_RemoveESPExit"
        stateName = "FTFT_ESP_Exit"
    elseif espType == "FreezePad" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/FleeTheFacility/ESP/ESP_FreezePod"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FleeTheFacility/ESP_REMOVE/ESP_RemoveESPFreezePod"
        stateName = "FTFT_ESP_FreezePad"
    elseif espType == "Name" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/FleeTheFacility/ESP/ESP_Name"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FleeTheFacility/ESP_REMOVE/ESP_RemoveESPName"
        stateName = "FTFT_ESP_Name"
    elseif espType == "Players" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/FleeTheFacility/ESP/ESP_Players"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FleeTheFacility/ESP_REMOVE/ESP_RemoveESPPlayers"
        stateName = "FTFT_ESP_Players"
    end
    
    States[stateName] = state
    
    if state then
        task.spawn(function()
            local success = pcall(function()
                local fullUrl = GetLatestGitHubScript(activateUrl)
                return loadstring(game:HttpGet(fullUrl, true))()
            end)
            
            if success then
                self["ESP_" .. espType .. "_Active"] = true
                
                CleanupSystem:RegisterScript("FTFT_ESP_" .. espType, function()
                    CleanupSystem:ExecuteLoadstringForCleanup(removeUrl)
                end)
            else
                self["ESP_" .. espType .. "_Active"] = false
            end
        end)
    else
        if self["ESP_" .. espType .. "_Active"] then
            task.spawn(function()
                local success = CleanupSystem:ExecuteLoadstringForCleanup(removeUrl)
                
                if success then
                    self["ESP_" .. espType .. "_Active"] = false
                else
                    self["ESP_" .. espType .. "_Active"] = false
                end
            end)
        end
    end
end

function FleeTheFacility:ToggleESP_AirVent(state)
    self:ToggleESP("AirVent", state)
end

function FleeTheFacility:ToggleESP_Beast(state)
    self:ToggleESP("Beast", state)
end

