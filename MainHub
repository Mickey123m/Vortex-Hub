-- [[ TOWER OF HELL SYSTEMS ]]
local TowerOfHell = {
    -- States
    ClickCTRL_Active = false,
    GodMode_Active = false,
    
    -- Loaded Scripts
    LoadedScripts = {}
}

-- Função para executar Teleport To Win (botão EXECUTE)
function TowerOfHell:ExecuteTeleportToWin()
    task.spawn(function()
        local success = pcall(function()
            local url = GetLatestGitHubScript("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/TowerofHell/TELEPORTS/TELEPORTS_TeleportToWin")
            loadstring(game:HttpGet(url, true))()
        end)
        
        if success then
            print("[Vortex Hub] Tower of Hell: Teleport To Win executado com sucesso!")
        else
            warn("[Vortex Hub] Tower of Hell: Erro ao executar Teleport To Win")
        end
    end)
end

-- Função para ativar/desativar Click CTRL TP
function TowerOfHell:ToggleClickCTRL(state)
    States.TOH_ClickCTRL = state
    
    if state then
        if not self.ClickCTRL_Active then
            task.spawn(function()
                local success = pcall(function()
                    local url = GetLatestGitHubScript("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/TowerofHell/UTILITIES/UTILITIES_ClickCTRLTp")
                    loadstring(game:HttpGet(url, true))()
                end)
                
                if success then
                    self.ClickCTRL_Active = true
                    print("[Vortex Hub] Tower of Hell: Click CTRL TP ativado!")
                    
                    CleanupSystem:RegisterScript("TOH_ClickCTRL", function()
                        CleanupSystem:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/TowerofHell/UTILITIES_REMOVE/UTILITIES_RemoveClickCTRLTp")
                    end)
                else
                    self.ClickCTRL_Active = false
                    warn("[Vortex Hub] Tower of Hell: Erro ao ativar Click CTRL TP")
                end
            end)
        end
    else
        if self.ClickCTRL_Active then
            task.spawn(function()
                local success = CleanupSystem:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/TowerofHell/UTILITIES_REMOVE/UTILITIES_RemoveClickCTRLTp")
                
                if success then
                    self.ClickCTRL_Active = false
                    print("[Vortex Hub] Tower of Hell: Click CTRL TP desativado!")
                else
                    self.ClickCTRL_Active = false
                    print("[Vortex Hub] Tower of Hell: Click CTRL TP desativado (fallback)")
                end
            end)
        end
    end
end

-- Função para ativar/desativar God Mode
function TowerOfHell:ToggleGodMode(state)
    States.TOH_GodMode = state
    
    if state then
        if not self.GodMode_Active then
            task.spawn(function()
                local success = pcall(function()
                    local url = GetLatestGitHubScript("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/TowerofHell/UTILITIES/UTILITIES_GodMode")
                    loadstring(game:HttpGet(url, true))()
                end)
                
                if success then
                    self.GodMode_Active = true
                    print("[Vortex Hub] Tower of Hell: God Mode ativado!")
                    
                    CleanupSystem:RegisterScript("TOH_GodMode", function()
                        CleanupSystem:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/TowerofHell/UTILITIES_REMOVE/UTILITIES_RemoveGodMode")
                    end)
                else
                    self.GodMode_Active = false
                    warn("[Vortex Hub] Tower of Hell: Erro ao ativar God Mode")
                end
            end)
        end
    else
        if self.GodMode_Active then
            task.spawn(function()
                local success = CleanupSystem:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/TowerofHell/UTILITIES_REMOVE/UTILITIES_RemoveGodMode")
                
                if success then
                    self.GodMode_Active = false
                    print("[Vortex Hub] Tower of Hell: God Mode desativado!")
                else
                    self.GodMode_Active = false
                    print("[Vortex Hub] Tower of Hell: God Mode desativado (fallback)")
                end
            end)
        end
    end
end

