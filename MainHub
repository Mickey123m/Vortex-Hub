-- Função para ativar/desativar AIMBOT
function Westbound:ToggleAimbot(aimbotType, state)
    local activateUrl = ""
    local removeUrl = ""
    local stateName = ""
    
    -- Mapear tipo para URLs
    if aimbotType == "AimbotE" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/Westbound/AIMBOT/AIMBOT_AimbotE"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/Westbound/AIMBOT_REMOVE/AIMBOT_RemoveAimbotE"
        stateName = "Westbound_AIMBOT_AimbotE"
    else
        warn("[Vortex Hub] Westbound: Tipo de Aimbot inválido: " .. tostring(aimbotType))
        return
    end
    
    -- Atualizar estado
    States[stateName] = state
    
    if state then
        -- Ativar Aimbot
        task.spawn(function()
            local success = pcall(function()
                local fullUrl = GetLatestGitHubScript(activateUrl)
                return loadstring(game:HttpGet(fullUrl, true))()
            end)
            
            if success then
                self["AIMBOT_" .. aimbotType .. "_Active"] = true
                print("[Vortex Hub] Westbound: Aimbot " .. aimbotType .. " ativado!")
                
                -- Registrar limpeza
                CleanupSystem:RegisterScript("Westbound_AIMBOT_" .. aimbotType, function()
                    CleanupSystem:ExecuteLoadstringForCleanup(removeUrl)
                end)
            else
                self["AIMBOT_" .. aimbotType .. "_Active"] = false
                warn("[Vortex Hub] Westbound: Erro ao ativar Aimbot " .. aimbotType)
            end
        end)
    else
        -- Desativar Aimbot
        if self["AIMBOT_" .. aimbotType .. "_Active"] then
            task.spawn(function()
                local success = CleanupSystem:ExecuteLoadstringForCleanup(removeUrl)
                
                if success then
                    self["AIMBOT_" .. aimbotType .. "_Active"] = false
                    print("[Vortex Hub] Westbound: Aimbot " .. aimbotType .. " desativado!")
                else
                    self["AIMBOT_" .. aimbotType .. "_Active"] = false
                    print("[Vortex Hub] Westbound: Aimbot " .. aimbotType .. " desativado (fallback)")
                end
            end)
        end
    end
