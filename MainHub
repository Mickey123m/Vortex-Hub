        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/MurderersVSSheriffsDUELS/AIMBOT/AIMBOT_AimCTRL"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/MurderersVSSheriffsDUELS/AIMBOT_REMOVE/AIMBOT_AimCTRL"
        stateName = "MVS_AIMBOT_AimCTRL"
    end
    
    States[stateName] = state
    
    if state then
        task.spawn(function()
            local success = pcall(function()
                local fullUrl = GetLatestGitHubScript(activateUrl)
                return loadstring(game:HttpGet(fullUrl, true))()
            end)
            
            if success then
                self["AIMBOT_" .. aimbotType .. "_Active"] = true
                
                CleanupSystem:RegisterScript("MVS_AIMBOT_" .. aimbotType, function()
                    CleanupSystem:ExecuteLoadstringForCleanup(removeUrl)
                end)
            else
                self["AIMBOT_" .. aimbotType .. "_Active"] = false
            end
        end)
    else
        if self["AIMBOT_" .. aimbotType .. "_Active"] then
            task.spawn(function()
                local success = CleanupSystem:ExecuteLoadstringForCleanup(removeUrl)
                
                if success then
                    self["AIMBOT_" .. aimbotType .. "_Active"] = false
                else
                    self["AIMBOT_" .. aimbotType .. "_Active"] = false
                end
            end)
        end
    end
end

function MurderersVSSheriffsDUELS:ToggleAimCTRL(state)
    self:ToggleAimbot("AimCTRL", state)
end

function MurderersVSSheriffsDUELS:ToggleUtility(utilityType, state)
    local activateUrl = ""
    local removeUrl = ""
    local stateName = ""
    
    if utilityType == "AutoKillPlayers" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/MurderersVSSheriffsDUELS/UTILITIES/UTILITIES_AutoKillPlayers"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/MurderersVSSheriffsDUELS/UTILITIES_REMOVE/UTILITIES_RemoveAutoKillPlayers"
        stateName = "MVS_UTILITIES_AutoKillPlayers"
    elseif utilityType == "FastFire" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/MurderersVSSheriffsDUELS/UTILITIES/UTILITIES_FastFire"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/MurderersVSSheriffsDUELS/UTILITIES_REMOVE/UTILITIES_RemoveFastFire"
        stateName = "MVS_UTILITIES_FastFire"
