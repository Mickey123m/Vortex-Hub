    ESP_Murder_Active = false,
    ESP_Sheriff_Active = false,
    
    AutoGun_Active = false,
    AutoKillAll_Active = false,
    AutoKillSheriff_Active = false
}

function MurderMystery2:ToggleESP(espType, state)
    local activateUrl = ""
    local removeUrl = ""
    local stateName = ""
    
    if espType == "All" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/MurderMystery2/ESP/ESP_All"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/MuderMystery2/ESP_REMOVE/ESP_RemoveESPAll"
        stateName = "MM2_ESP_All"
    elseif espType == "GunDrop" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/MurderMystery2/ESP/ESP_GunDrop"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/MuderMystery2/ESP_REMOVE/ESP_RemoveESPGunDrop"
        stateName = "MM2_ESP_GunDrop"
    elseif espType == "Murder" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/MurderMystery2/ESP/ESP_Murder"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/MuderMystery2/ESP_REMOVE/ESP_RemoveESPMurder"
        stateName = "MM2_ESP_Murder"
    elseif espType == "Sheriff" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/MurderMystery2/ESP/ESP_Sheriff"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/MuderMystery2/ESP_REMOVE/ESP_RemoveESPSheriff"
        stateName = "MM2_ESP_Sheriff"
    end
    
    States[stateName] = state
    
    if state then
        task.spawn(function()
            local success = pcall(function()
                local fullUrl = GetLatestGitHubScript(activateUrl)
                return loadstring(game:HttpGet(fullUrl, true))()
            end)
            
            if success then
                self["ESP_" .. espType .. "_Active"] = true
                
                CleanupSystem:RegisterScript("MM2_ESP_" .. espType, function()
                    CleanupSystem:ExecuteLoadstringForCleanup(removeUrl)
                end)
            else
                self["ESP_" .. espType .. "_Active"] = false
            end
        end)
    else
        if self["ESP_" .. espType .. "_Active"] then
            task.spawn(function()
                local success = CleanupSystem:ExecuteLoadstringForCleanup(removeUrl)
                
                if success then
                    self["ESP_" .. espType .. "_Active"] = false
                else
                    self["ESP_" .. espType .. "_Active"] = false
                end
            end)
        end
    end
end

