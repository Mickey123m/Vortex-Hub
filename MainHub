-- Função genérica para ativar/desativar ESPs do Westbound
function Westbound:ToggleESP(espType, state)
    local activateUrl = ""
    local removeUrl = ""
    local stateName = ""
    
    -- Mapear tipo para URLs
    if espType == "Players" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/Westbound/ESP/ESP_Players"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/Westbound/ESP_REMOVE/ESP_RemoveESPPlayers"
        stateName = "Westbound_ESP_Players"
    elseif espType == "Animals" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/Westbound/ESP/ESP_Animals"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/Westbound/ESP_REMOVE/ESP_RemoveESPAnimals"
        stateName = "Westbound_ESP_Animals"
    elseif espType == "DistanceEName" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/Westbound/ESP/ESP_DistanceEName"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/Westbound/ESP_REMOVE/ESP_RemoveESPDistanceEName"
        stateName = "Westbound_ESP_DistanceName"
    elseif espType == "Name" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/Westbound/ESP/ESP_Name"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/Westbound/ESP_REMOVE/ESP_RemoveESPName"
        stateName = "Westbound_ESP_Name"
    elseif espType == "Health" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/Westbound/ESP/ESP_Health"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/Westbound/ESP_REMOVE/ESP_RemoveESPHealth"
        stateName = "Westbound_ESP_Health"
    elseif espType == "NameAnimals" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/Westbound/ESP/ESP_NameAnimals"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/Westbound/ESP_REMOVE/ESP_RemoveESPNameAnimals"
        stateName = "Westbound_ESP_NameAnimals"
    else
        warn("[Vortex Hub] Westbound: Tipo de ESP inválido: " .. tostring(espType))
        return
    end
    
    -- Atualizar estado
    States[stateName] = state
    
    if state then
        -- Ativar ESP
        task.spawn(function()
            local success = pcall(function()
                local fullUrl = GetLatestGitHubScript(activateUrl)
                return loadstring(game:HttpGet(fullUrl, true))()
            end)
            
            if success then
                self["ESP_" .. espType .. "_Active"] = true
                print("[Vortex Hub] Westbound: ESP " .. espType .. " ativado!")
                
                -- Registrar limpeza
                CleanupSystem:RegisterScript("Westbound_ESP_" .. espType, function()
                    CleanupSystem:ExecuteLoadstringForCleanup(removeUrl)
                end)
            else
                self["ESP_" .. espType .. "_Active"] = false
                warn("[Vortex Hub] Westbound: Erro ao ativar ESP " .. espType)
            end
        end)
    else
        -- Desativar ESP
        if self["ESP_" .. espType .. "_Active"] then
            task.spawn(function()
                local success = CleanupSystem:ExecuteLoadstringForCleanup(removeUrl)
                
                if success then
                    self["ESP_" .. espType .. "_Active"] = false
                    print("[Vortex Hub] Westbound: ESP " .. espType .. " desativado!")
                else
                    self["ESP_" .. espType .. "_Active"] = false
                    print("[Vortex Hub] Westbound: ESP " .. espType .. " desativado (fallback)")
                end
            end)
        end
    end
end

