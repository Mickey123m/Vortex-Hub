        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/MurderersVSSheriffsDUELS/UTILITIES/UTILITIES_IncreaseHitbox"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/MurderersVSSheriffsDUELS/UTILITIES_REMOVE/UTLITIES_RemoveIncreaseHitbox"
        stateName = "MVS_UTILITIES_IncreaseHitbox"
    end
    
    States[stateName] = state
    
    if state then
        task.spawn(function()
            local success = pcall(function()
                local fullUrl = GetLatestGitHubScript(activateUrl)
                return loadstring(game:HttpGet(fullUrl, true))()
            end)
            
            if success then
                self["UTILITIES_" .. utilityType .. "_Active"] = true
                
                CleanupSystem:RegisterScript("MVS_Utility_" .. utilityType, function()
                    CleanupSystem:ExecuteLoadstringForCleanup(removeUrl)
                end)
            else
                self["UTILITIES_" .. utilityType .. "_Active"] = false
            end
        end)
    else
        if self["UTILITIES_" .. utilityType .. "_Active"] then
            task.spawn(function()
                local success = CleanupSystem:ExecuteLoadstringForCleanup(removeUrl)
                
                if success then
                    self["UTILITIES_" .. utilityType .. "_Active"] = false
                else
                    self["UTILITIES_" .. utilityType .. "_Active"] = false
                end
            end)
        end
    end
end

