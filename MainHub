-- [[ BLIND SHOT SYSTEMS (ATUALIZADO PARA LOADSTRINGS) ]]
local BlindShot = {
    ESP_Players_Active = false,
    AutoFarm_Active = false
}

function BlindShot:ToggleESP_Players(state)
    States.BlindShot_ESP = state
    
    if state then
        if not self.ESP_Players_Active then
            task.spawn(function()
                local success = pcall(function()
                    local url = GetLatestGitHubScript("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/BlindShot/ESP/ESP_Players")
                    loadstring(game:HttpGet(url, true))()
                end)
                
                if success then
                    self.ESP_Players_Active = true
                    print("[Vortex Hub] Blind Shot: ESP Players ativado!")
                    
                    CleanupSystem:RegisterScript("BlindShotESPPlayers", function()
                        CleanupSystem:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/BLINDSHOT/ESP_REMOVE/ESP_RemoveESPPlayers")
                    end)
                else
                    self.ESP_Players_Active = false
                    warn("[Vortex Hub] Blind Shot: Erro ao ativar ESP Players")
                end
            end)
        end
    else
        if self.ESP_Players_Active then
            task.spawn(function()
                local success = CleanupSystem:ExecuteLoadstringForCleanup("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/BLINDSHOT/ESP_REMOVE/ESP_RemoveESPPlayers")
                
                if success then
                    self.ESP_Players_Active = false
                    print("[Vortex Hub] Blind Shot: ESP Players desativado!")
                else
                    self.ESP_Players_Active = false
                    print("[Vortex Hub] Blind Shot: ESP Players desativado (fallback)")
                end
            end)
        end
    end
end

