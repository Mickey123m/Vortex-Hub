    local activateUrl = ""
    local removeUrl = ""
    local stateName = ""
    
    if utilityType == "AntiFall" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/OneOfUs/UTILITIES/UTILITIES_AntiFall"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/UTILITIES_REMOVE/UTILITIES_RemoveAntiFall"
        stateName = "OOU_UTILITIES_AntiFall"
    elseif utilityType == "AntiTrap" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/OneOfUs/UTILITIES/UTILITIES_AntiTrap"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/UTILITIES_REMOVE/UTILITIES_RemoveAntiTrap"
        stateName = "OOU_UTILITIES_AntiTrap"
    elseif utilityType == "AutoGeradors" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/OneOfUs/UTILITIES/UTILITIES_AutoGeradors"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/UTILITIES_REMOVE/UTILITIES_RemoveAutoGeradors"
        stateName = "OOU_UTILITIES_AutoGeradors"
    elseif utilityType == "AutoHealPlayers" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/OneOfUs/UTILITIES/UTILITIES_AutoHealPlayers"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/UTILITIES_REMOVE/UTILITIES_RemoveAutoHealPlayers"
        stateName = "OOU_UTILITIES_AutoHealPlayers"
    elseif utilityType == "AutoInteractions" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/OneOfUs/UTILITIES/UTILITIES_AutoInteractions"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/UTILITIES_REMOVE/UTILITIES_RemoveInteractions"
        stateName = "OOU_UTILITIES_AutoInteractions"
    elseif utilityType == "AutoSaboteGeradors" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/OneOfUs/UTILITIES/UTILITIES_AutoSaboteGeradors"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/UTILITIES_REMOVE/UTILITIES_RemoveAutoSaboteGeradors"
        stateName = "OOU_UTILITIES_AutoSaboteGeradors"
    elseif utilityType == "DistanceGeradors" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/OneOfUs/UTILITIES/UTILITIES_DistanceGeradors"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/UTILITIES_REMOVE/UTILITIES_RemoveDistanceGeradors"
        stateName = "OOU_UTILITIES_DistanceGeradors"
    elseif utilityType == "Noclip" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/OneOfUs/UTILITIES/UTILITIES_Noclip"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/OneOfUs/UTILITIES_REMOVE/UTILITIES_RemoveNoclip"
        stateName = "OOU_UTILITIES_Noclip"
    elseif utilityType == "FullBrightness" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/FNAFEternalNights/UTILITIES/PLAYER_FullBrightness"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/FNAFEternalNights/UTILITIES_REMOVE/UTILITIES_RemoveFullBrightness"
        stateName = "OOU_UTILITIES_FullBrightness"
    end
    
    States[stateName] = state
    
    if state then
        task.spawn(function()
            local success = pcall(function()
                local fullUrl = GetLatestGitHubScript(activateUrl)
                return loadstring(game:HttpGet(fullUrl, true))()
            end)
            
            if success then
                self["UTILITIES_" .. utilityType .. "_Active"] = true
                
                CleanupSystem:RegisterScript("OOU_Utility_" .. utilityType, function()
                    CleanupSystem:ExecuteLoadstringForCleanup(removeUrl)
                end)
            else
                self["UTILITIES_" .. utilityType .. "_Active"] = false
            end
        end)
    else
        if self["UTILITIES_" .. utilityType .. "_Active"] then
            task.spawn(function()
                local success = CleanupSystem:ExecuteLoadstringForCleanup(removeUrl)
                
                if success then
                    self["UTILITIES_" .. utilityType .. "_Active"] = false
                else
                    self["UTILITIES_" .. utilityType .. "_Active"] = false
                end
            end)
        end
    end
end

