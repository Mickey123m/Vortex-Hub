function Westbound:ToggleWorld(worldType, state)
    local activateUrl = ""
    local removeUrl = ""
    local stateName = ""
    
    -- Mapear tipo para URLs
    if worldType == "DisableFog" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/Westbound/WORLD/WORLD_DisableFog"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/Westbound/WORLD_REMOVE/World_RemoveDisableFog"
        stateName = "Westbound_WORLD_DisableFog"
    elseif worldType == "DisableJumpLimite" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/Westbound/WORLD/WORLD_DisableJumpLimite"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/Westbound/WORLD_REMOVE/WORLD_RemoveDisableJumpLimit"
        stateName = "Westbound_WORLD_DisableJumpLimite"
    elseif worldType == "EspItemsHouse" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/Westbound/WORLD/WORLD_EspItemsHouse"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/Westbound/WORLD_REMOVE/WORLD_RemoveEspItemsHouse"
        stateName = "Westbound_WORLD_EspItemsHouse"
    elseif worldType == "InstantRob" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/Westbound/WORLD/WORLD_InstantRob"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/Westbound/WORLD_REMOVE/WORLD_RemvoeInstantRob"
        stateName = "Westbound_WORLD_InstantRob"
    elseif worldType == "NoFallDamage" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/Westbound/WORLD/WORLD_NoFallDamage"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/Westbound/WORLD_REMOVE/WORLD_RemoveNoFallDamage"
        stateName = "Westbound_WORLD_NoFallDamage"
    else
        warn("[Vortex Hub] Westbound: Tipo de World inválido: " .. tostring(worldType))
        return
    end
    
    -- Atualizar estado
    States[stateName] = state
    
    if state then
        -- Ativar World
        task.spawn(function()
            local success = pcall(function()
                local fullUrl = GetLatestGitHubScript(activateUrl)
                return loadstring(game:HttpGet(fullUrl, true))()
            end)
            
            if success then
                self["WORLD_" .. worldType .. "_Active"] = true
                print("[Vortex Hub] Westbound: World " .. worldType .. " ativado!")
                
                -- Registrar limpeza
                CleanupSystem:RegisterScript("Westbound_WORLD_" .. worldType, function()
                    CleanupSystem:ExecuteLoadstringForCleanup(removeUrl)
                end)
            else
                self["WORLD_" .. worldType .. "_Active"] = false
                warn("[Vortex Hub] Westbound: Erro ao ativar World " .. worldType)
            end
        end)
    else
        -- Desativar World
        if self["WORLD_" .. worldType .. "_Active"] then
            task.spawn(function()
                local success = CleanupSystem:ExecuteLoadstringForCleanup(removeUrl)
                
                if success then
                    self["WORLD_" .. worldType .. "_Active"] = false
                    print("[Vortex Hub] Westbound: World " .. worldType .. " desativado!")
                else
                    self["WORLD_" .. worldType .. "_Active"] = false
                    print("[Vortex Hub] Westbound: World " .. worldType .. " desativado (fallback)")
                end
            end)
        end
    end
end

