        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/MurderersVSSheriffsDUELS/ESP_REMOVE/ESP_RemoveESPPlayers"
        stateName = "MVS_ESP_Players"
    else
        return
    end
    
    States[stateName] = state
    
    if state then
        task.spawn(function()
            local success = pcall(function()
                local fullUrl = GetLatestGitHubScript(activateUrl)
                return loadstring(game:HttpGet(fullUrl, true))()
            end)
            
