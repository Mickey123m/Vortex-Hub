    UTILITIES_FastFire_Active = false,
    UTILITIES_IncreaseHitbox_Active = false,
}

function MurderersVSSheriffsDUELS:ToggleESP(espType, state)
    local activateUrl = ""
    local removeUrl = ""
    local stateName = ""
    
    if espType == "Hitbox" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/MurderersVSSheriffsDUELS/ESP/ESP_Hitbox"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/MurderersVSSheriffsDUELS/ESP_REMOVE/ESP_RemoveESPHitbox"
        stateName = "MVS_ESP_Hitbox"
    elseif espType == "Name" then
        activateUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/MurderersVSSheriffsDUELS/ESP/ESP_Name"
        removeUrl = "https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Models/MurderersVSSheriffsDUELS/ESP_REMOVE/ESP_RemoveName"
        stateName = "MVS_ESP_Name"
