    self:ToggleESP("FreezePad", state)
end

function FleeTheFacility:ToggleESP_Name(state)
    self:ToggleESP("Name", state)
end

function FleeTheFacility:ToggleESP_Players(state)
    self:ToggleESP("Players", state)
end

