    self:ToggleESP("Sheriff", state)
end

-- Função para executar Teleport Lobby (botão executar)
function MurderMystery2:ExecuteTeleportLobby()
    task.spawn(function()
        local success = pcall(function()
            local url = GetLatestGitHubScript("https://raw.githubusercontent.com/Mickey123m/Vortex-Hub/refs/heads/main/Games/MurderMystery2/TELEPORTS/TELEPORTS_TeleportLobby")
            loadstring(game:HttpGet(url, true))()
        end)
        
        if success then
            print("[Vortex Hub] Murder Mystery 2: Teleport Lobby executado com sucesso!")
        else
            warn("[Vortex Hub] Murder Mystery 2: Erro ao executar Teleport Lobby")
        end
    end)
end

